{
  "name": "testdemoproject",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr",
    "build": "ng build --prod && mv dist/testdemoproject _static",
    "build:production": "export NODE_ENV=production && ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n:extract": "npx locl extract -s=./dist/**/*.js -f=json -o=src/assets/i18n/en.json",
    "i18n:extract-with-build": "npm run build:prod && npm run i18n:extract",
    "i18n:run": "node scripts/i18n.js",
    "i18n:check": "node scripts/i18n-check.js",
    "i18n:xliff": "xliffmerge --profile scripts/xliffmerge.json",
    "i18n:convert": "npx locl convert -s=i18n/messages.en.xlf -f=json -o=src/assets/i18n/en.json && npx locl convert -s=i18n/messages.nl.xlf -f=json -o=src/assets/i18n/nl.json",
    "i18n:watch": "onchange 'i18n/messages.*.xlf' -- npm run i18n:convert",
    "project:convert": "node scripts/rename.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.2",
    "@angular/common": "~11.0.2",
    "@angular/compiler": "~11.0.2",
    "@angular/core": "~11.0.2",
    "@angular/forms": "~11.0.2",
    "@angular/localize": "^11.0.2",
    "@angular/platform-browser": "~11.0.2",
    "@angular/platform-browser-dynamic": "~11.0.2",
    "@angular/router": "~11.0.2",
    "@locl/core": "^1.0.1",
    "ng-zorro-antd": "^10.1.2",
    "ngx-build-plus": "^10.1.1",
    "ngx-date-fns": "^6.6.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular/cli": "~11.0.2",
    "@angular/compiler-cli": "~11.0.2",
    "@locl/cli": "^1.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ngx-i18nsupport": "^0.17.1",
    "postcss": "^8.1.7",
    "postcss-import": "^13.0.0",
    "postcss-loader": "^4.0.4",
    "postcss-scss": "^3.0.4",
    "prettier": "^2.1.2",
    "protractor": "~7.0.0",
    "readline-sync": "^1.4.10",
    "replace-in-file": "^6.1.0",
    "tailwindcss": "^1.9.6",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.0.2"
  }
}
